{
  "name": "museamp",
  "version": "1.0.0-alpha.1",
  "description": "a music player using electron",
  "main": "src/main-process/main.js",
  "scripts": {
    "babel:watch": "babel src --out-dir lib -s -w",
    "start": "yarn run babel:watch & yarn run run:dev",
    "run:dev": "NODE_ENV=development electron src/main-process/main.js",
    "test": "tape -r @babel/register './src/**/*.spec.js'",
    "test:coverage": "nyc tape -r @babel/register './src/**/*.spec.js' | tap-nyc",
    "postinstall": "./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihgrant/museamp.git"
  },
  "author": "ihgrant",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-hot-loader": "^0.8.0",
    "electron-rebuild": "^1.10.1",
    "flow-bin": "^0.121.0",
    "nyc": "^15.0.0",
    "prettier": "^1.5.3",
    "should": "^11.2.1",
    "tap-nyc": "^1.0.3",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "classnames": "^2.2.5",
    "electron": "1",
    "express": "^4.15.4",
    "findit": "^2.0.0",
    "howler": "^2.2.1",
    "icepick": "^2.1.1",
    "lodash": "^4.17.15",
    "musicmetadata": "^2.0.4",
    "react": "^16.14",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^16.14",
    "react-electron-menu": "^0.3.0",
    "react-photonkit": "https://github.com/ihgrant/react-photonkit.git",
    "react-redux": "^5.1",
    "react-treeview": "^0.4.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "sequelize": "5",
    "sqlite3": "^4.1.1"
  }
}
