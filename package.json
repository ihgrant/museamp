{
  "name": "museamp",
  "version": "1.0.0-alpha.1",
  "description": "a music player using electron",
  "main": "main.js",
  "scripts": {
    "babel:watch": "babel src --out-dir lib -s -w",
    "start": "npm run babel:watch & npm run run:dev",
    "run:dev": "NODE_ENV=development electron src/main-process/main.js",
    "test": "tape -r babel-register test/**/*.js | tap-spec",
    "postinstall": "./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihgrant/museamp.git"
  },
  "author": "ihgrant",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.24.1",
    "electron-rebuild": "^1.1.5",
    "flow-bin": "^0.48.0",
    "should": "^9.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "classnames": "^2.2.5",
    "electron": "^1.6.2",
    "findit": "^2.0.0",
    "icepick": "^1.3.0",
    "lodash": "^4.13.1",
    "musicmetadata": "^2.0.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-photonkit": "^0.4.1",
    "react-redux": "^5.0.4",
    "react-treeview": "^0.4.5",
    "redux": "^3.6.0",
    "sequelize": "^3.23.3",
    "sqlite3": "^3.1.4"
  }
}
