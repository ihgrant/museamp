{
  "name": "museamp",
  "version": "1.0.0-alpha.1",
  "description": "a music player using electron",
  "main": "src/main-process/main.js",
  "scripts": {
    "babel:watch": "babel src --out-dir lib -s -w",
    "start": "npm run babel:watch & npm run run:dev",
    "run:dev": "NODE_ENV=development electron src/main-process/main.js",
    "test": "tape -r babel-register test/**/*.js | tap-spec",
    "postinstall": "./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihgrant/museamp.git"
  },
  "author": "ihgrant",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.6.0",
    "flow-bin": "^0.121.0",
    "prettier": "^1.5.3",
    "should": "^11.2.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "classnames": "^2.2.5",
    "electron": "^1.8.8",
