    "icepick": "^2.1.1",
    "lodash": "^4.13.1",
    "musicmetadata": "^2.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-electron-menu": "^0.3.0",
    "react-photonkit": "^0.4.1",
    "react-redux": "^5.0.6",
